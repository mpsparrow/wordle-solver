<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="ttk.Frame" id="mainwindow">
    <property name="height">500</property>
    <property name="width">500</property>
    <layout manager="grid">
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Label" id="title_label">
        <property name="anchor">n</property>
        <property name="font">{qtquickcontrols} 20 {}</property>
        <property name="justify">center</property>
        <property name="takefocus">false</property>
        <property name="text" translatable="yes">Wordle Solver</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relwidth">0.4</property>
          <property name="relx">0.3</property>
          <property name="rely">0.0</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="open_button">
        <property name="command" type="command" cbtype="simple">on_open_button</property>
        <property name="text" translatable="yes">OPEN</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relwidth">0.15</property>
          <property name="relx">0.045</property>
          <property name="rely">0.7</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="start_button">
        <property name="command" type="command" cbtype="simple">on_start_button</property>
        <property name="text" translatable="yes">START</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relwidth">0.15</property>
          <property name="relx">0.235</property>
          <property name="rely">0.7</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="close_button">
        <property name="command" type="command" cbtype="simple">on_close_button</property>
        <property name="text" translatable="yes">CLOSE</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relwidth">0.15</property>
          <property name="relx">0.805</property>
          <property name="rely">0.7</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Progressbar" id="progress_bar">
        <property name="length">10</property>
        <property name="maximum">10</property>
        <property name="mode">determinate</property>
        <property name="orient">horizontal</property>
        <property name="value">0</property>
        <property name="variable">int:progress_var</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relwidth">0.9</property>
          <property name="relx">0.05</property>
          <property name="rely">0.78</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Labelframe" id="auto_frame">
        <property name="height">200</property>
        <property name="text" translatable="yes">Auto Solver</property>
        <property name="width">200</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relheight">0.3</property>
          <property name="relwidth">0.90</property>
          <property name="relx">0.05</property>
          <property name="rely">0.07</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
        <child>
          <object class="ttk.Scale" id="speed_scale">
            <property name="from_">1</property>
            <property name="length">100</property>
            <property name="orient">horizontal</property>
            <property name="to">100</property>
            <property name="value">50</property>
            <property name="variable">int:speed_var</property>
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="relwidth">0.3</property>
              <property name="relx">0.15</property>
              <property name="rely">0.1</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="speed_label">
            <property name="text" translatable="yes">Speed</property>
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="relx">0.05</property>
              <property name="rely">0.1</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Labelframe" id="manual_frame">
        <property name="height">200</property>
        <property name="text" translatable="yes">Manual Solver</property>
        <property name="width">200</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relheight">0.3</property>
          <property name="relwidth">0.90</property>
          <property name="relx">0.05</property>
          <property name="rely">0.375</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="pause_button">
        <property name="command" type="command" cbtype="simple">on_pause_button</property>
        <property name="text" translatable="yes">PAUSE</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relwidth">0.15</property>
          <property name="relx">0.425</property>
          <property name="rely">0.7</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="stop_button">
        <property name="command" type="command" cbtype="simple">on_stop_button</property>
        <property name="text" translatable="yes">STOP</property>
        <layout manager="place">
          <property name="anchor">nw</property>
          <property name="relwidth">0.15</property>
          <property name="relx">0.615</property>
          <property name="rely">0.7</property>
          <property name="x">0</property>
          <property name="y">0</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
